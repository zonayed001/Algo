#include <stdio.h>
int main()
{int n=6;
int a[n];
int i=0;
for(int i=0;i<n;i++){
        printf(" \n");
    scanf("%d",&a[i]);
}
 printf(" Array=");
for(int i=0;i<n;i++){

    printf(" %d",a[i]);
}
int min =a[0];
for(int i=0;i<n;i++){
    if(min>a[i]){
        min=a[i];
    }
}

int max =a[0];

for(int i=0;i<n;i++){
    if(max<a[i]){
        max=a[i];
    }
}
printf("\nMin array =%d",min);
printf("\nMin array =%d",max);
}
