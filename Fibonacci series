#include <stdio.h>
#include<stdbool.h>
#include<conio.h>
int fib(int n )
{
    if(n==0 || n==1)
    {
        return 1;
    }
    else
        return fib(n-1)+fib (n-2);


}
int main()
{
    int n=20;
    int array[n];
    for(int i=0; i<n; i++)
    {
        array[i]=fib(i);

    }
for(int i=0;i<n;i++){

    printf(" %d",array[i]);
}
}
